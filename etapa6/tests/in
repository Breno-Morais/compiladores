int x = 10;
bool y = false;

int main() {
    y = 50 < 40;
    print "less" y;

    y = 50 > 40;
    print "greater" y;

    y = 50 >= 50;
    print "greater equal" y;
    
    y = 50 <= 40;
    print "less equal" y;
}
