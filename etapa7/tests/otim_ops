int i = 1;

int main() {

    // Vira shift

    i = i * 2;
    i = i * 4;
    i = i * 5;
    i = i / 2;
    i = i / 4;
    i = i / 5;

    // Constant Fold
    i = 5 + 5;
    i = 1 - 1;
    i = 6 / 3;
    i = 2 * 3;
    i = 10 % 4;

    i = i * 2;
    i = 4 * i;
    i = i * 8;

    i = i / 2;
    i = 2 / i;
    i = i / 4;
}