int main() {
    print "less 2 < 1 is " ( 2 < 1 ) "\n";
    print "less 1 < 1 is " ( 1 < 1 ) "\n";
    print "less 1 < 2 is " ( 1 < 2 ) "\n";

    print "greater 2 > 1 is " ( 2 > 1 ) "\n";
    print "greater 1 > 1 is " ( 1 > 1 ) "\n";
    print "greater 1 > 2 is " ( 1 > 2 ) "\n";

    print "greater or equal 2 >= 1 is " ( 2 >= 1 ) "\n";
    print "greater or equal 1 >= 1 is " ( 1 >= 1 ) "\n";
    print "greater or equal 1 >= 2 is " ( 1 >= 2 ) "\n";

    print "less or equal 2 <= 1 is " ( 2 <= 1 ) "\n";
    print "less or equal 1 <= 1 is " ( 1 <= 1 ) "\n";
    print "less or equal 1 <= 2 is " ( 1 <= 2 ) "\n";
}
