int x = 5;
int y = 2;

int main() {
    if(x == y) {
        print "5 is equal to 2\n";
    } else {
        print "5 is not equal to 2\n";
    }

    print "x is " x "\n";

    while(x > 2) {
        print "x is still higher than 2 because it is " x "\n";
        x = x - 1;
    }

    print "x is now " x "\n";

    if(x == y) {
        print "5 is equal to 2\n";
    } else {
        print "5 is not equal to 2\n";
    }

    read x;

    print "Você digitou " x "\n";

    if(x == 1) {
        print "parabéns\n";
    } 
}
